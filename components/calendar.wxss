@font-face {
    font-family: 'iconfont';
    src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
    font-weight: normal;
    font-style: normal;
}
.iconfont {
  font-family:"iconfont" !important;
  font-size:16px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.rightArrow:before { content: "\e604"; }
.leftArrow:before { content: "\e605"; }
.FLEX{
  display: flex;
  display: -webkit-box;}
.MAINCENTER{
  -webkit-box-pack:center;
  -webkit-justify-content:center;
  justify-content:center;
}
.CROSSCENTER{
  -webkit-box-align:center;
  -webkit-align-items:center;
  align-items:center;
}
.CalendarHead{
  height: 80rpx;
  border-bottom: 1rpx solid #eaeaea;
  font-size: 40rpx;
}
.CalendarHead button{width: auto;display: inline-block;padding: 0;margin: 0 30rpx;background: transparent;}
.CalendarHead button::after,.CalendarBody button::after{display: none;background: transparent;}
.CalendarHead button,.CalendarBody button{background-color: transparent!important;}
.CalendarHead view{width: 50vw;}
.leftArrow,.rightArrow{font-size: 40rpx;}
.weekBox{height: 80rpx;font-size: 28rpx;color: orange;display:-webkit-box;display: box;}
.weekBox text{line-height: 80rpx;-webkit-box-flex: 1;box-flex: 1;display: inherit;-webkit-box-pack:center;-webkit-justify-content:center;justify-content:center;}
.CalendarBody{height: auto;flex-wrap:wrap;-webkit-flex-wrap:wrap;width: 100vw;display: flex;}
.CalendarBody button{height: 100rpx;width: 14.2857vw;color: black;display: inherit;-webkit-box-pack:center;-webkit-justify-content:center;justify-content:center;background: transparent;padding: 10rpx 0 0 0;position: relative;border-bottom: 1px solid #eaeaea;}
.CalendarBody .DATE{display: inline-block;font-size: 30rpx;line-height: 10rpx;height: 30rpx;padding: 10rpx;border-radius: 50%;width: 30rpx;}
.CalendarBody .DATE.nowDate{color: white;background: #007aff;}
.CalendarBody .DATE.tag{background: #ff3f85;color:white;}
.BtmTxt{position: absolute;padding: 2rpx 0;text-align: center;background: #333;font-size: 20rpx;color: white;left: 0;right: 0;bottom: 0;}